{
  "name": "avl-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@availabs/ams": "^1.0.9",
    "@availabs/avl-components": "^1.0.58",
    "@draft-js-plugins/editor": "^4.1.0",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "d3-format": "^2.0.0",
    "d3-time-format": "^3.0.0",
    "draft-js": "^0.11.7",
    "html-to-draftjs": "^1.5.0",
    "linkify-it": "^3.0.2",
    "lodash": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.3",
    "tlds": "^1.219.0",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "npm run build:styles && react-scripts start",
    "build": "react-scripts build",
    "build:styles": "tailwind build src/styles/index.css -o src/styles/tailwind.css",
    "deploy": "react-scripts build && aws s3 sync build/ s3://shmp",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "import/no-anonymous-default-export": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.9",
    "tailwindcss": "^2.1.1"
  }
}
